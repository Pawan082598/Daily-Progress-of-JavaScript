console.log("Hello Everyone");

//   Var
var message;
message="Hello";
console.log(message);

var num=30;
num=38;  // reassigning the value of num 
console.log(num);

// Let

let num1=54;
num1=67;  // reassigning the value of num1
console.log(num1);

// Const
const num2=90;
 num2=100;  // cannot reassign the value of num2
 console.log(num2);


